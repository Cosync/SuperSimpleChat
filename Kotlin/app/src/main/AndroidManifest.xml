<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cosync.supersimplechatandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SuperSimpleChatAndroid">
        <activity
            android:name="com.cosync.supersimplechatandroid.chat.ChatActivity"
            android:label="@string/title_activity_chat"
            android:theme="@style/Theme.SuperSimpleChatAndroid.NoActionBar"></activity>
        <activity
            android:name="com.cosync.supersimplechatandroid.main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.SuperSimpleChatAndroid.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>